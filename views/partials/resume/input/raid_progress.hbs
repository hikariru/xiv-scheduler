<div class="col-md-6">
  <h5 class="md-3">攻略状況</h5>
  {{# each raids }}
    <h6 class="mt-3">{{ title }}</h6>
    <div class="row ms-3 mb-3 g-3">
      {{# each volumes }}
        <div class="col-md form-check">
          <input class="form-check-input" type="checkbox" id="raid-progress-{{ id }}" name="raid-progress[]"
                 value="{{ id }}">
          <label class="form-check-label" for="raid-progress-{{ id }}">
            {{ title }}
          </label>
        </div>
      {{/each}}
    </div>
  {{/each}}
</div>
